                      <td width="100%" height="20">&nbsp;<img src="{$=#global_images_route}public/small/sico.gif" border="0">&nbsp;<a href="{$=#nuri}">{$=ii_itake('global.lng_admin.admin_sort_all','lng')}</a>{$nav_sort}</td>
                    </tr>
                    <tr>
                      <td width="100%" height="5"></td>
