      <tr>
        <td class="tbt">{$=ii_itake('config.manage', 'lng')}</td>
      </tr>
      <tr>
        <td><img src="{$=#images_route}icon/icon2.gif" border="0" class="absmiddle" />&nbsp;<a href="?type=manage&amp;mtype=topic&amp;sid={$sid}">{$=ii_itake('config.manage_topic', 'lng')}</a>&nbsp;&nbsp;<img src="{$=#images_route}icon/icon2.gif" border="0" class="absmiddle" />&nbsp;<a href="?type=manage&amp;mtype=blacklist&amp;sid={$sid}">{$=ii_itake('config.manage_blacklist', 'lng')}</a></td>
      </tr>
    </table>
]]></tpl_default>
    </item>
    <item>
      <name><![CDATA[data_forum_info]]></name>
      <tpl_default><![CDATA[{@recurrence_ida}
            <table width="100%" cellpadding="0" cellspacing="0" border="0">
              <tr>
                <td height="20" colspan="3"><img src="{$=#images_route}icon/topic.gif" border="0" /> <a href="?type=detail&sid={$sid}&tid={$tid}" target="_blank">{$topic}</a></td>
              </tr>
              <tr>
                <td height="20" colspan="3"><img src="{$=#images_route}icon/time.gif" border="0" /> {$time}</td>
              </tr>
              <tr>
                <td width="33%" height="20"><img src="{$=#images_route}icon/num_new.gif" border="0" class="absmiddle" /> {$num_new}</td>
                <td width="33%"><img src="{$=#images_route}icon/num_topic.gif" border="0" class="absmiddle" /> {$num_topic}</td>
                <td width="33%"><img src="{$=#images_route}icon/num_note.gif" border="0" class="absmiddle" /> {$num_note}</td>
              </tr>
            </table>{@@}
            <table width="100%" cellpadding="0" cellspacing="0" border="0">
              <tr>
                <td align="center" class="ash">{$=ii_itake('module.popedom', 'lng')}</td>
              </tr>
            </table>{@recurrence_ida}]]></tpl_default>
