    <item>
      <name><![CDATA[js_urls_set_date]]></name>
      <tpl_default><![CDATA[<script type="text/javascript">
function urls_set_date()
{
  var tobj, tvalue;
  tobj = get_id("count");
  tvalue = get_num(tobj.value);
  if (tvalue < 0) tvalue = 0;
  var tdobj, tdvalue;
  tdobj = get_id("urls_date_option");
  tdvalue = get_num(tdobj.value);
  if (tdvalue < 0) tdvalue = 0;
  if (tvalue && tdvalue)
  {
    var tnums = tvalue - tdvalue ;
    if (tnums < 0)
    {
      for (i = tdvalue; i > tvalue; i--)
      {
        if (get_id("urls_date_option" + i))
        {
          get_id("urls_date_option" + i).style.display = "none";
        }
      }
    }
    else
    {
      var tobjdate, tobjstr;
      tobjdate = get_id("urls_date");
      tobjstr = ""
      for (i = tdvalue; i <= tvalue; i++)
      {
        if (get_id("urls_date_option" + i))
        {
          get_id("urls_date_option" + i).style.display = "";
        }
        else
        {
          tobjstr += "<table id=\"urls_date_option" + i + "\" border=\"0\" width=\"100%\" cellSpacing=\"0\" cellPadding=\"0\"><tr><td width=\"80\" height=\"25\">{$=ii_itake('config.urls_topic', 'lng')}[" + i + "]</td><td width=\"*\"><input type=\"text\" name=\"urls_topic" + i + "\" size=\"20\"></td></tr><tr><td height=\"25\">{$=ii_itake('config.urls_link', 'lng')}[" + i + "]</td><td width=\"*\"><input type=\"text\" name=\"urls_link" + i + "\" size=\"40\"></td></tr></table>"
        }
      }
      tobjdate.innerHTML += tobjstr;
    }
    tdobj.value = tvalue;
  }
}
</script>]]></tpl_default>
