    <item>
      <name><![CDATA[index]]></name>
      <tpl_default><![CDATA[
{$=mm_web_head($GLOBALS['nhead'])}
<div id="middle">
  <div id="middleContent2">{$=pp_module_data_top()}{@recurrence_forum}
    <table cellpadding="5" cellspacing="1" class="tables">
      <tr>
        <td class="tbt" colspan="3">{$sort}</td>
      </tr>{@recurrence_ida}
      <tr>
        <td width="60" align="center"><img src="{$=#images_route}state/{$pic}.gif" border="0" /></td>
        <td>
          <table width="100%" cellpadding="0" cellspacing="0" border="0">
            <tr>
              <td height="20"><a href="./?type=list&amp;sid={$id}" target="_self"><b>{$sort}</b></a></td>
              <td rowspan="3" align="right">{$forum_images}</td>
            </tr>
            <tr>
              <td class="ash">{$explain}</td>
            </tr>
