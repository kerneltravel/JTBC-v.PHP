      <tpl_default><![CDATA[{$=ii_ireplace('manage.js_urls_set_date', 'tpl')}
<script language="javascript" src="{$=#global_images_route}js/preview_images.js"></script>
<script language="javascript" src="{$=#global_images_route}js/loader.js"></script>
<script language="javascript">
function bfsubmit()
{
  get_id("content_images_list").value = get_selects_list(get_id('content_images'));
}

function change_bsize(strt, strvalue)
{
  var tvalue = get_num(strvalue)
  var tbvalue, tkbvalue, tmbvalue
  switch (strt)
  {
    case "b":
      tbvalue = tvalue;
      tkbvalue = tvalue / 1024;
      tmbvalue = tvalue / (1024*1024);
      break;
    case "kb":
      tbvalue = tvalue * 1024;
      tkbvalue = tvalue;
      tmbvalue = tvalue / 1024;
      break;
    case "mb":
      tbvalue = tvalue * 1024 * 1024;
      tkbvalue = tvalue * 1024;
      tmbvalue = tvalue;
      break;
    default :
  }
  get_id("size").value = tbvalue;
  get_id("kbsize").value = tkbvalue;
  get_id("mbsize").value = tmbvalue;
}
</script>
