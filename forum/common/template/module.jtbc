    <item>
      <name><![CDATA[topic_list]]></name>
      <tpl_default><![CDATA[
{$=mm_web_head($GLOBALS['nhead'])}
<script type="text/javascript">
var mystrsid, mystrtid, mystrarea;

function loadreply(strsid, strtid, strarea)
{
  mystrsid = strsid;
  mystrtid = strtid;
  mystrarea = strarea;
  switch_display("tr_" + strarea);
  if (!(get_id("tr_" + strarea).style.display == "none"))
  {igets("interface.php?type=reply&sid=" + mystrsid + "&tid=" + mystrtid, nloadreply);}
};

function nloadreply(strers)
{
  var tobj, tstr;
  tstr = strers;
  tobj = get_id("td_" + mystrarea);
  if (!(tstr == "$error$"))
  {tobj.innerHTML = tstr;}
};
</script>
<div id="middle">
  <div id="middleContent2">{$=pp_module_data_top()}
    <table cellpadding="5" cellspacing="1" class="tables">
      <tr>
        <td class="tbt">{$=ii_itake('module.rule', 'lng')}</td>
      </tr>
      <tr>
        <td>{$rule}</td>
      </tr>
    </table>{@child_forum}
    <table cellpadding="5" cellspacing="1" class="tables">
      <tr>
        <td class="tbt" colspan="3">{$sort}</td>
      </tr>{@recurrence_ida}
      <tr>
        <td width="60" align="center"><img src="{$=#images_route}state/{$pic}.gif" border="0" /></td>
        <td>
          <table width="100%" cellpadding="0" cellspacing="0" border="0">
            <tr>
              <td height="20"><a href="./?type=list&amp;sid={$id}" target="_self"><b>{$sort}</b></a></td>
              <td rowspan="3" align="right">{$forum_images}</td>
            </tr>
            <tr>
              <td>{$explain}</td>
            </tr>
            <tr>
              <td height="20">{$=ii_itake('config.admin', 'lng')}: {$admin}</td>
            </tr>
          </table>
        </td>
        <td width="250">{$forum_info}</td>
      </tr>{@recurrence_ida}
    </table>{@child_forum}{@topic_list}
    <table cellpadding="0" cellspacing="0" class="tables">
      <tr>
        <td class="tbn"><a href="./?type=release&amp;sid={$sid}"><img src="{$=#images_route}post/topic_{$=#nlng}.gif" alt="{$=ii_itake('module.newtopic', 'lng')}" class="absmiddle" /></a>&nbsp;&nbsp;&nbsp;<a href="?type=release&sid={$sid}&vote=1"><img src="{$=#images_route}post/vote_{$=#nlng}.gif" alt="{$=ii_itake('module.newvote', 'lng')}" class="absmiddle" /></a></td>
        <td class="tbn" align="right"><a href="./?type=manage&amp;sid={$sid}"><img src="{$=#images_route}icon/admin.gif" border="0" class="absmiddle" /></a> {$forum_admin}</td>
      </tr>
    </table>
    <table cellpadding="5" cellspacing="1" class="tables">
      <tr>
        <td class="tbt" colspan="7">{$=ii_itake('module.topiclist', 'lng')}</td>
      </tr>
      <tr>
        <td width="3%" align="center"><img src="{$=#images_route}topic/ico.gif" class="absmiddle" /></td>
        <td width="3%" align="center"><img src="{$=#images_route}topic/ico.gif" class="absmiddle" /></td>
        <td width="47%">{$=ii_itake('config.topic', 'lng')}</td>
        <td width="13%">{$=ii_itake('config.author', 'lng')}</td>
        <td width="7%" align="center">{$=ii_itake('config.reply', 'lng')}</td>
        <td width="7%" align="center">{$=ii_itake('config.count', 'lng')}</td>
        <td width="20%">{$=ii_itake('config.lastreply', 'lng')}</td>
      </tr>{@recurrence_idb}
      <tr>
        <td align="center"><img src="{$=#images_route}topic/{$ico}.gif" class="absmiddle" /></td>
        <td align="center"><a href="javascript:loadreply('{$sid}','{$tid}','{$tid}');"><img src="{$=#images_route}tp/{$icon}.gif" border="0" class="absmiddle" /></a></td>
        <td><a href="./?type=detail&amp;sid={$sid}&amp;tid={$tid}">{$topic}</a> {$topicpic}</td>
        <td><a href="{$=ii_get_actual_route(USER_FOLDER)}/?type=user_detail&amp;username={$=urlencode('{$author}')}">{$author}</a></td>
        <td align="center">{$reply}</td>
        <td align="center">{$count}</td>
        <td>{$lasttime} <a href="{$=ii_get_actual_route(USER_FOLDER)}/?type=user_detail&amp;username={$=urlencode('{$lastuser}')}">{$lastuser}</a></td>
      </tr>
      <tr id="tr_{$tid}" style="display:none;">
        <td colspan="2">&nbsp;</td>
        <td colspan="5" id="td_{$tid}"><img src="{$=#images_route}loading.gif" alt="Loading" class="absmiddle" />{$=ii_itake('module.loadreply', 'lng')}</td>
      </tr>{@recurrence_idb}
      <tr>
        <td colspan="7"><div id="cutepage" class="cutepage">{$cpagestr}</div></td>
      </tr>
    </table>
    <table cellpadding="0" cellspacing="0" class="tables">
      <tr>
        <td width="25"><img src="{$=#images_route}topic/normal.gif" border="0" /></td>
        <td width="70">{$=ii_itake('topic.normal', 'lng')}</td>
        <td width="27"><img src="{$=#images_route}topic/hot.gif" border="0" /></td>
        <td width="70">{$=ii_itake('topic.hot', 'lng')}</td>
        <td width="25"><img src="{$=#images_route}topic/elite.gif" border="0" /></td>
        <td width="70">{$=ii_itake('topic.elite', 'lng')}</td>
        <td width="25"><img src="{$=#images_route}topic/lock.gif" border="0" /></td>
        <td width="70">{$=ii_itake('topic.lock', 'lng')}</td>
        <td width="25"><img src="{$=#images_route}topic/top.gif" border="0" /></td>
        <td width="70">{$=ii_itake('topic.top', 'lng')}</td>
        <td width="25"><img src="{$=#images_route}topic/htop.gif" border="0" /></td>
        <td width="70">{$=ii_itake('topic.htop', "lng")}</td>
        <td align="right">
          <form method="get">
          <input type="hidden" name="type" value="search" />
          <input type="hidden" name="sid" value="{$sid}" />
          <input type="text" name="keyword" style="WIDTH: 120px" class="text" /> <input type="submit" value="{$=ii_itake('global.lng_config.search','lng')}" class="button" />
          </form>
        </td>
      </tr>
    </table>{@topic_list}
  </div>
</div>
{$=mm_web_foot($GLOBALS['nfoot'])}
]]></tpl_default>
    </item>
    <item>
      <name><![CDATA[topic_detail]]></name>
      <tpl_default><![CDATA[
{$=mm_web_head($GLOBALS['nhead'])}
<script type="text/javascript">
var mystrarea;

function forum_quote(strsid, strtid, strarea)
{
  mystrarea = strarea;
  igets("interface.php?type=quote&sid=" + strsid + "&tid=" + strtid, forum_quotes)
};

function forum_quotes(strers)
{
  var tstr = strers;
  if (!(tstr == "$error$"))
  {itextner(mystrarea, tstr);}
};
</script>
<div id="middle">
  <div id="middleContent2">{$=pp_module_data_top()}{@recurrence_vote}
    <form action="{$=#nuri}?action=vote&id={$id}&backurl={$=urlencode($GLOBALS['nurl'])}" method="post">
    <table cellpadding="5" cellspacing="1" class="tables">
      <tr>
        <td class="tbt">{$=ii_itake('module.votedetail', 'lng')}</td>
      </tr>
      <tr>
        <td>{$topic}</td>
      </tr>
      <tr>
        <td valign="top">
          <table width="100%" cellpadding="0" cellspacing="0" border="0">{@recurrence_voa}
            <tr>
              <td width="30" height="20" align="center"><input type="{$=pp_change_forum_vote_type({$type})}" name="votes[]" value="{$id}" class="radio"></td>
              <td>{$topic}</td>
              <td width="450">
                <table width="{$per}%" cellpadding="0" cellspacing="0" border="0" style="margin-left:0px;">
                  <tr>
                    <td width="{$per}%" height="10" style="background: url({$=#images_route}/bar.gif)"></td>
                  </tr>
               </table>
             </td>
             <td width="40" align="center">{$per}%</td>
            </tr>{@recurrence_voa}
          </table>
        </td>
      </tr>
      <tr>
        <td valign="top">
          <table width="100%" cellpadding="0" cellspacing="0" border="0">
            <tr>
              <td>{$=ii_itake('vote.all_1', 'lng')}<b>{$count}</b>{$=ii_itake('vote.all_2', 'lng')}{$endtime}</td>
              <td align="right"><input type="submit" value="{$=ii_itake('vote.submit', 'lng')}" class="button" /></td>
            </tr>
          </table>
        </td>
      </tr>
    </table>
    </form>{@recurrence_vote}
    <table cellpadding="5" cellspacing="1" class="tables">
      <tr>
        <td class="tbt" colspan="2">{$=ii_itake('module.topicdetail', 'lng')}</td>
      </tr>
      <tr>{@recurrence_ida}
        <td width="140" valign="top">
          <table width="100%" cellpadding="0" cellspacing="0" border="0">
            <tr>
              <td height="20" class="ash">{$floor} {$=ii_itake('global.lng_unit.floor', 'lng')}</td>
            </tr>
            <tr>
              <td height="20"><img src="{$=ii_get_actual_route(USER_FOLDER)}/{$=#images_route}group/{$utype}.gif" class="absmiddle" /> <a href="{$=ii_get_actual_route(USER_FOLDER)}/?type=user_detail&amp;username={$=urlencode('{$username}')}" target="_blank"><u>{$username}</u></a></td>
            </tr>
            <tr>
              <td height="5"></td>
            </tr>
            <tr>
              <td><img src="{$face}" border="0" onload="if ({$face_u}) {this.width = {$face_width} ; this.height = {$face_height} ;}" /></td>
            </tr>
            <tr>
              <td height="5"></td>
            </tr>
          </table>
          <table width="100%" cellpadding="0" cellspacing="0" border="0">
            <tr>
              <td width="40" height="20">{$=ii_itake('global.lng_noun.topic', 'lng')}</td>
              <td>{$num_topic}</td>
            </tr>
            <tr>
              <td height="20">{$=ii_itake('global.lng_noun.integral', 'lng')}</td>
              <td>{$integral}</td>
            </tr>
            <tr>
              <td height="20">{$=ii_itake('config.reg', 'lng')}</td>
              <td>{$regtime}</td>
            </tr>
          </table>
        </td>
        <td valign="top">
          <table width="100%" cellpadding="0" cellspacing="0" border="0">
            <tr>
              <td width="50" height="25"><a href="{$=ii_get_actual_route(USER_FOLDER)}/?type=user_detail&amp;username={$=urlencode('{$username}')}"><img src="{$=#images_route}icon/profile.gif" /></a><td>
              <td width="40"><a href="{$=ii_get_actual_route(USER_FOLDER)}/message/?type=send&amp;username={$=urlencode('{$username}')}"><img src="{$=#images_route}icon/mail.gif" /></a></td>
              <td width="40"><a href="mailto:{$email}"><img src="{$=#images_route}icon/email.gif" / ></a></td>
              <td width="40"><a href="./?type=edit&amp;tid={$tid}&amp;sid={$sid}&amp;backurl={$=urlencode($GLOBALS['nurl'])}"><img src="{$=#images_route}icon/edit.gif" border="0" /></a></td>
              <td><a href="javascript:forum_quote('{$sid}','{$tid}','content');"><img src="{$=#images_route}icon/quote.gif" border="0" /></a></td>
            </tr>
          </table>
          <table width="100%" cellpadding="0" cellspacing="0" border="0" style="table-layout:fixed">
            <tr>
              <td height="25"><img src="{$=#images_route}tp/{$icon}.gif" border="0" class="absmiddle" /> <b>{$topic}</b></td>
            </tr>
            <tr>
              <td height="10"></td>
            </tr>
            <tr>
              <td><div class="contents">{$content}</div></td>
            </tr>
          </table>
        </td>
      </tr>
      <tr>
        <td><img src="{$=#images_route}icon/time.gif" class="absmiddle" /> {$time}</td>
        <td><img src="{$=#images_route}icon/sign.gif" class="absmiddle" /> {$sign}</td>
      </tr>{@recurrence_ida}
      <tr>
        <td colspan="2"><div id="cutepage" class="cutepage">{$cpagestr}</div></td>
      </tr>
    </table>
    <table cellpadding="5" cellspacing="1" class="tables">
      <tr>
        <td colspan="2" class="tbt">{$=ii_itake('module.topicreply', 'lng')}</td>
      </tr>
      <tr>
        <td valign="top">{@topic_reply}
          <form action="{$=#nuri}?action=reply&tid={$tid}&sid={$sid}&backurl={$=urlencode($GLOBALS['nurl'])}" method="post">
          <table width="100%" cellpadding="0" cellspacing="0" border="0">
            <tr>
              <td width="80" height="25">{$=ii_itake('config.replytopic', 'lng')}</td>
              <td><input type="text" name="topic" size="40" maxlength="50" /></td>
            </tr>
            <tr>
              <td height="25"{$=ii_itake('config.mood', 'lng')}</td>
              <td>{$=ii_show_xmlinfo_select('sel_mood.all', 0, 'icon:radio')}</td>
            </tr>
            <tr>
              <td height="35">{$=ii_itake('global.lng_config.ubb', 'lng')}</td>
              <td>{$=mm_ubb_bar('content')}</td>
            </tr>
            <tr>
              <td height="25"><font class="red">*</font> {$=ii_itake('config.replycontent', 'lng')}</td>
              <td><textarea rows="8" name="content" id="content" cols="75" ondblclick='this.rows+=3;' onblur='this.rows=8;' class="textarea"></textarea></td>
            </tr>
            <tr>
              <td height="35">{$=ii_itake('global.lng_config.em', 'lng')}</td>
              <td>{$=mm_em_bar('content')}</td>
            </tr>
            <tr>
              <td height="25">{$=ii_itake('config.topicmode', 'lng')}</td>
              <td>{$=ii_show_xmlinfo_select('sel_topicmode.all', 1, 'ubb:radio')}</td>
            </tr>{@recurrence_valcode}
            <tr>
              <td height="25">{$=ii_itake('global.lng_config.valcode', 'lng')}</td>
              <td><input type="text" name="valcode" size="4" /> {$=mm_valcode()}</td>
            </tr>{@recurrence_valcode}
            <tr>
              <td height="35"></td>
              <td><input type="submit" name="submit" value="{$=ii_itake('global.lng_config.submit','lng')}" class="button" />&nbsp;<input type="reset" name="reset" value="{$=ii_itake('global.lng_config.reset','lng')}" class="button" /></td>
            </tr>
          </form>
          </table>{@@}
          <table width="100%" cellpadding="0" cellspacing="0" border="0">
            <tr>
              <td class="red">{$=ii_itake('config.lockinfo', 'lng')}</td>
            </tr>
          </table>{@topic_reply}
        </td>
      </tr>
    </table>
  </div>
</div>
{$=mm_web_foot($GLOBALS['nfoot'])}
]]></tpl_default>
    </item>
    <item>
      <name><![CDATA[topic_release]]></name>
      <tpl_default><![CDATA[
{$=mm_web_head($GLOBALS['nhead'])}
{$=mm_echo_error()}
<script type="text/javascript" src="{$=#global_images_route}js/loader.js"></script>
<script type="text/javascript" src="{$=#global_images_route}js/selects.js"></script>
<script type="text/javascript">
function bfsubmit()
{
  {@recurrence_user_upload_script}get_id("content_files_list").value = get_selects_list(get_id('content_files'));{@recurrence_user_upload_script}
};
</script>
<div id="middle">
  <div id="middleContent2">{$=pp_module_data_top()}
