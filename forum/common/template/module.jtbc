  <item_list>
    <item>
      <name><![CDATA[data_login]]></name>
      <tpl_default><![CDATA[{@recurrence_ida}
    <form action="{$=ii_get_actual_route(USER_FOLDER)}/index.php?action=login&backurl={$=urlencode($GLOBALS['nurl'])}" method="post">
    <table width="100%" cellpadding="0" cellspacing="0" border="0">
      <tr>
        <td>{$=ii_itake('global.lng_config.username','lng')} <input type="text" name="username" size="20" class="text" />&nbsp;{$=ii_itake('global.lng_config.password','lng')} <input type="password" name="password" size="20" class="text" />{@recurrence_valcode} {$=ii_itake('global.lng_config.valcode', 'lng')} <input type="text" name="valcode" size="4" class="text" /> {$=mm_valcode()}{@recurrence_valcode}&nbsp;<input type="submit" value="{$=ii_itake('global.' . USER_FOLDER . ':api.login','lng')}" class="button" /> <img src="{$=#global_images_route}public/small/sico.gif" border="0" /> <a href="{$=ii_get_actual_route(USER_FOLDER)}">{$=ii_itake('global.' . USER_FOLDER . ':api.register_newuser','lng')}</a> <img src="{$=#global_images_route}public/small/sico.gif" border="0" /> <a href="{$=ii_get_actual_route(USER_FOLDER)}/?type=lostpassword">{$=ii_itake('global.' . USER_FOLDER . ':api.lost_password', 'lng')}</a></td>
      </tr>
    </table>
    </form>{@@}
    <table width="100%" cellpadding="0" cellspacing="0" border="0">
      <tr>
        <td>{$=#nusername}, <a href="{$=ii_get_actual_route(USER_FOLDER)}/?type=manage">{$=ii_itake('global.' . USER_FOLDER . ':api.membercenter','lng')}</a>, <a href="{$=ii_get_actual_route(USER_FOLDER)}/message/?type=list" target="_self">{$=str_replace('[]', '[<font class="red">' . ap_count_user_message($GLOBALS['nusername']) . '</font>]', ii_itake('global.' . USER_FOLDER . ':api.message', 'lng'))}</a> <a href="{$=ii_get_actual_route(USER_FOLDER)}/?action=logout&backurl={$=urlencode($GLOBALS['nurl'])}" onclick="return click_return('{$=ii_itake('global.' . USER_FOLDER . ':api.logoutstr', 'lng')}')">{$=ii_itake('global.' . USER_FOLDER . ':api.logout','lng')}</a></td>
      </tr>
    </table>{@recurrence_ida}
]]></tpl_default>
    </item>
    <item>
      <name><![CDATA[data_top]]></name>
      <tpl_default><![CDATA[
    <table cellpadding="5" cellspacing="1" class="tables">
      <tr>
        <td class="tbt">{$=vv_inavigation($GLOBALS['ngenre'], 'strers=module;strurl=' . $GLOBALS['ngenre'])}{$=pp_nav_forum()}</td>
      </tr>
      <tr>
        <td>{$=ap_user_login('module.data_login')}</td>
      </tr>
    </table>
]]></tpl_default>
    </item>
    <item>
      <name><![CDATA[data_foot]]></name>
      <tpl_default><![CDATA[
    <table cellpadding="5" cellspacing="1" class="tables">
      <tr>
        <td class="tbt">{$=ii_itake('module.img', 'lng')}</td>
      </tr>
      <tr>
        <td align="center"><img src="{$=#images_route}state/forum.gif" border="0" class="absmiddle" />&nbsp;{$=ii_itake('module.forum', 'lng')}&nbsp;<img src="{$=#images_route}state/forum_new.gif" border="0" class="absmiddle" />&nbsp;{$=ii_itake('module.forum_new', 'lng')}&nbsp;<img src="{$=#images_route}state/forum_lock.gif" border="0" class="absmiddle" />&nbsp;{$=ii_itake('module.forum_lock', 'lng')}</td>
      </tr>
    </table>
]]></tpl_default>
    </item>
    <item>
      <name><![CDATA[data_manage_menu]]></name>
      <tpl_default><![CDATA[
